		SELECT * FROM  item where itemType =1
	</select>
	<select id="findCpuList" resultType="Item">
		SELECT * FROM  item where itemType =2
	</select>
	<select id="findClothList" resultType="Item">
		SELECT * FROM  item where itemType =3
	</select>
	<select id="findHomeeList" resultType="Item">
		SELECT * FROM  item where itemType =4
	</select>
	<select id="findFoodList" resultType="Item">
		SELECT * FROM  item where itemType =5
	</select>
	<select id="findBookList" resultType="Item">
		SELECT * FROM  item where itemType =6
	</select>
	<select id="findMedicalList" resultType="Item">
		SELECT * FROM  item where itemType =7
	</select>

	
	
</mapper>